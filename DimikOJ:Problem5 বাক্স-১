#include<stdio.h>
int main()
{
    int T,N,k,i,j;
    scanf("%d",&T);
    for(k=1; k<=T; k++)
    {
        scanf("%d",&N);
        for(i=1; i<=N; i++)
        {
            for(j=1; j<=N; j++)
            {
                printf("*");
            }
            printf("\n");
        }
    }
    return 0;
}
